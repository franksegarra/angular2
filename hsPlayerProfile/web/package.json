{
  "name": "hsstudentprofile",
  "version": "1.0.0",
  "author": "Frank Segarra",
  "description": "Application for high school student profiles.",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "clean": "gulp clean",
    "compile_ts": "gulp compile_ts",
    "bundle": "gulp bundle",
    "build": "npm run clean && tsc",
    "build_prod": "gulp dist",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "@types/jquery": "^2.0.40",
    "bootstrap": "^3.3.6",
    "jquery": "^3.1.1",
    "ng2-bootstrap": "1.1.16-11",
    "primeng": "^2.0.2",
    "reflect-metadata": "^0.1.10",
    "systemjs": "0.19.27"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.0",
    "typescript": "^2.2.1",
    "typings": "^1.0.4",
    "gulp": "^3.9.1", 
    "gulp-clean": "^0.3.2", 
    "gulp-html-replace": "^1.6.1", 
    "gulp-shell": "^0.5.2",
    "run-sequence": "^1.2.2",
    "systemjs-builder": "^0.15.31",
    "@types/jquery": "^2.0.40",
    "tslint": "^3.7.4",
    "browserify": "^13.0.1",
    "uglifyjs": "^2.4.10"
  },
  "repository": {}
}
